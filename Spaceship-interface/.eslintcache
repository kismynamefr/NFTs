en'));\n\n    const { pathname } = useLocation();\n    const [requestedLocation, setRequestedLocation] = useState();\n    if (!isLoggedIn) {\n        if (pathname !== requestedLocation) setRequestedLocation(pathname);\n        return <Login />\n    }\n\n    if (requestedLocation && pathname !== requestedLocation) {\n        setRequestedLocation(null);\n        return <Redirect to={requestedLocation} />;\n\n    }\n\n    return <>{children}</>\n}","/Users/spacedev-alex/Desktop/Spaceship-interface/src/view/Home/index.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/view/Login/index.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/view/Import-wallet/index.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/view/Select-action/index.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/components/Footer/index.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/utils/index.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/utils/history.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/components/Header/index.jsx",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/api/userApi.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/api/authApi.js",[],"/Users/spacedev-alex/Desktop/Spaceship-interface/src/redux/sagas/auth.js",["274"],"import authApi from \"../../api/authApi\";\nimport { push } from \"connected-react-router\";\nimport { call, delay, put, takeLatest } from \"redux-saga/effects\";\nimport { authActions } from \"../slices/auth\";\nimport { toast } from \"react-toastify\";\n\n\nfunction* handleLogin(action) {\n  try {\n    const response = yield call(authApi.authenticate, action.payload);\n    localStorage.setItem(\"uid_token\", response.firebaseUser.uid);\n    yield put(authActions.loginSuccess(response));\n    yield window.location.replace(`/user/uid=${response.firebaseUser.uid}`);\n  } catch (err) {\n    toast.error(err.response.data.message, {\n      position: toast.POSITION.BOTTO